<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/secondaryProgress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270" >
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="40.0"
                android:useLevel="true" >
                <gradient
                    android:centerColor="#cfcfcf"
                    android:endColor="#cfcfcf"
                    android:startColor="#cfcfcf"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270" >
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="40.0"
                android:useLevel="true" >
                <gradient
                    android:centerColor="#D73F0D"
                    android:endColor="#D73F0D"
                    android:startColor="#D73F0D"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>

