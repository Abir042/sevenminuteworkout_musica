        <activity
            android:name="freaktemplate.sevenminworkout.Setting"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>

        <service
            android:name="freaktemplate.sevenminworkout.MyAlarmService"
            android:enabled="true" />

        <receiver android:name="freaktemplate.sevenminworkout.MyReceiver" />

